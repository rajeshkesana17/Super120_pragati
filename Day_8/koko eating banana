mport math
class Solution:
    def minEatingSpeed(self, piles: List[int], h: int) -> int:
        l,r=1,max(piles)
        while l<=r:
            mid=(l+r)//2
            hour=0
            for banana in piles:
                hour+=math.ceil(banana/mid)
            if hour <= h:
                r=mid-1
            else:
                l=mid+1
        return l
