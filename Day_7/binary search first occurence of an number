
def binary_search(arr,key):
    l,r =0,len(arr)-1
    res=-1
    while l<=r:
        mid = (l+r)//2
        if arr[mid]==key:
            res= mid
            r=mid-1
        elif arr[mid] < key:
            l=mid+1
        else:r=mid-1
    return res
arr=[1,2,3,3,3,4,17,20]
res=binary_search(arr,3)
print(res)
